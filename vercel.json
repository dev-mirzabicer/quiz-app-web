{
    "version": 2,
    "name": "quiz-app-backend",
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "src/server.js",
            "headers": {
                "Access-Control-Allow-Origin": "https://dev-mirzabicer.github.io",
                "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type, Authorization"
            }
        }
    ]
}